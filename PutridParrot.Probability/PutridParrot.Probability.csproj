<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<Company>PutridParrot</Company>
	<Authors>Mark Timmings</Authors>
	<Description>Probability class/library</Description>
	<Version>1.0.0-alpha</Version>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<RepositoryUrl>https://github.com/putridparrot/PutridParrot.Probability.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<PackageIcon>PutridParrot.Probability.png</PackageIcon>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<AssemblyVersion>1.0.0.0</AssemblyVersion>
	<FileVersion>1.0.0.0</FileVersion>
  </PropertyGroup>

	<ItemGroup>
		<None Include="..\PutridParrot.Probability.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
